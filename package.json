{
  "name": "playground-react",
  "version": "0.0.1",
  "description": "playground-react 代码演示台",
  "author": "zcq",
  "homepage": "https://github.com/Jerryqun/playground-react",
  "keywords": [
    "playground",
    "playground-react"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Jerryqun/playground-react"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "scripts": {
    "start": "dumi dev",
    "docs:build": "dumi build",
    "build": "father-build",
    "postinstall": "patch-package"
  },
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
