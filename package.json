    "react-dom": "17.0.2"
  },
  "peerDependencies": {
    "antd": "4.20.0",
    "axios": "^0.21.1",
    "react": "17.0.2"
  },
  "devDependencies": {
    "@babel/runtime": "7.16.0",
    "@types/lodash": "4.14.168",
    "@types/react": "17.0.2",
    "@umijs/test": "^3.0.5",
    "antd": "4.20.0",
    "axios": "^0.21.1",
    "babel-plugin-import": "1.13.3",
    "dumi": "^1.0.14",
    "eslint": "^7.27.0",
    "eslint-plugin-react": "^7.26.1",
    "father-build": "^1.17.2",
    "gh-pages": "^3.0.0",
    "less": "2.7.3",
    "lint-staged": "^10.0.7",
    "prettier": "^2.2.1",
    "yorkie": "^2.0.0"
  },
  "license": "MIT"
}
